public class factorail_tailing_tozero {
    public static int factorail_tailing(int n) {
        if (n == 0) {
            return 0;
        }
        int factorof5 = n / 5;
        return factorof5 + factorail_tailing(n / 5);
    }

    public static void main(String[] args) {
        int n = 10;
        int result = factorail_tailing(n);
        System.out.println(result);
    }
}
