
import java.util.Stack;

public class Backspace_String_Compare {

    public static String string_compare(String str) {
        char[] chars = str.toCharArray();
        Stack<Character> st = new Stack<>();
        for (int i = 0; i < chars.length; i++) {
            if (chars[i] != '#') {
                st.push(chars[i]);
            } else {
                if (!st.isEmpty()) {
                    st.pop();
                }
            }
        }
        String update_String = st.toString();
        return update_String;

    }

    public static boolean check_same(String s, String t) {
        String update_s = string_compare(s);
        String update_t = string_compare(t);

        if (update_s.equals(update_t)) {
            return true;
        }
        return false;
    }

    public static void main(String[] args) {
        String s = "a#c";
        String t = "b";
        Boolean isSame = check_same(s, t);
        System.out.println("Both the string  s,t  are same: " + isSame);
    }
}



output :------------
Both the string  s,t  are same: false
